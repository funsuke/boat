{
	"width": 1280,
	"height": 720,
	"fps": 30,
	"main": "./script/main.js",
	"assets": {
		"main": {
			"type": "script",
			"path": "script/main.js",
			"global": true
		},
		"button": {
			"type": "image",
			"path": "image/button.png",
			"width": 800,
			"height": 400,
			"global": true
		},
		"title": {
			"type": "image",
			"path": "image/title.png",
			"width": 1280,
			"height": 720,
			"global": true
		},
		"ControllerTitle": {
			"type": "script",
			"path": "script/controller/ControllerTitle.js",
			"global": true
		},
		"params": {
			"type": "script",
			"path": "script/params.js",
			"global": true
		},
		"SceneEntry": {
			"type": "script",
			"path": "script/SceneEntry.js",
			"global": true
		},
		"SceneGetName": {
			"type": "script",
			"path": "script/SceneGetName.js",
			"global": true
		}
	},
	"moduleMainScripts": {
		"@akashic-extension/coe": "node_modules/@akashic-extension/coe/lib/index.js",
		"@akashic-extension/akashic-hover-plugin": "node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js",
		"@akashic-extension/resolve-player-info": "node_modules/@akashic-extension/resolve-player-info/lib/index.js"
	},
	"environment": {
		"sandbox-runtime": "3",
		"nicolive": {
			"supportedModes": [
				"multi_admission"
			]
		},
		"external": {
			"coeLimited": "0",
			"atsumaru": "0"
		}
	},
	"globalScripts": [
		"node_modules/@akashic-extension/coe/lib/Controller.js",
		"node_modules/@akashic-extension/coe/lib/SceneWatcher.js",
		"node_modules/@akashic-extension/coe/lib/View.js",
		"node_modules/@akashic-extension/coe/lib/global.js",
		"node_modules/@akashic-extension/coe/lib/impl/BaseController.js",
		"node_modules/@akashic-extension/coe/lib/impl/COEController.js",
		"node_modules/@akashic-extension/coe/lib/impl/COEMessageEventHandler.js",
		"node_modules/@akashic-extension/coe/lib/impl/Scene.js",
		"node_modules/@akashic-extension/coe/lib/impl/index.js",
		"node_modules/@akashic-extension/coe/lib/index.js",
		"node_modules/@akashic-extension/coe/lib/parameters.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/Converter.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/HoverPlugin.js",
		"node_modules/@akashic-extension/akashic-hover-plugin/lib/index.js",
		"node_modules/@akashic-extension/resolve-player-info/lib/FallbackDialog.js",
		"node_modules/@akashic-extension/resolve-player-info/lib/index.js"
	]
}